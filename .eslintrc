{
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "rules": {
    "comma-dangle": [1, "never"], // 不对数组或对象末尾逗号做强制要求
    "spaced-comment": [0, "always"], // 不对注释前的空格做强制要求
    "func-names": 0, // 允许使用匿名函数
    "react/prefer-stateless-function": 0,
    "react/prefer-es6-class": 1,
    "linebreak-style": [0],
    "max-len": [1, 120]
  }
}
